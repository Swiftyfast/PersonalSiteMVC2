
@{
    ViewBag.Title = "Portfolio";
}
<br />
<br />
<br />
<!-- Portfolio Section-->
<section class="page-section portfolio" id="portfolio">
    @*<div class="container">*@
    <div class="myContainer">
        <!-- Portfolio Section Heading-->
        <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Portfolio</h2>
        <!-- Icon Divider-->
        <div class="divider-custom">
            <div class="divider-custom-line"></div>
            <div class="divider-custom-icon"><i class="fa fa-star" aria-hidden="true"></i></div>
            <div class="divider-custom-line"></div>
        </div>
        <!-- Portfolio Grid Items-->
        <div class="row">
            <!-- Portfolio Item Final Project -->
            <div class="col-md-6 col-lg-4">
                <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal7">
                    <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                        <div class="portfolio-item-caption-content text-center text-white"><i class="fa fa-plus fa-3x" aria-hidden="true"></i></div>
                    </div>
                    <img class="img-fluid" src="~/Content/assets/img/finalProjectAvi.jpg" alt="A fancy chess board" style="border:1px solid black" />
                </div>
            </div>
            <!-- Portfolio Item Redside KC-->
            <div class="col-md-6 col-lg-4 mb-5">
                <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal2">
                    <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                        <div class="portfolio-item-caption-content text-center text-white"><i class="fa fa-plus fa-3x" aria-hidden="true"></i></div>
                    </div>
                    <img class="img-fluid" src="~/Content/assets/img/residekc_logo_200.png" alt="" />
                </div>
            </div>
            <!-- Portfolio Item Dungeon App-->
            <div class="col-md-6 col-lg-4 mb-5 mb-lg-0">
                <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal4">
                    <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                        <div class="portfolio-item-caption-content text-center text-white"><i class="fa fa-plus fa-3x" aria-hidden="true"></i></div>
                    </div>
                    <img class="img-fluid" src="~/Content/assets/img/boat.jpg" alt="" />
                </div>
            </div>
            <!-- Portfolio Item Storefront -->
            <div class="col-md-6 col-lg-4">
                <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal6">
                    <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                        <div class="portfolio-item-caption-content text-center text-white"><i class="fa fa-plus fa-3x" aria-hidden="true"></i></div>
                    </div>
                    <img class="img-fluid" src="~/Content/assets/img/storefrontChess.jfif" alt="A fancy chess board" />
                </div>
            </div>
            <!-- Portfolio Item ALD Map -->
            <div class="col-md-6 col-lg-4 mb-5">
                <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal3">
                    <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                        <div class="portfolio-item-caption-content text-center text-white"><i class="fa fa-plus fa-3x" aria-hidden="true"></i></div>
                    </div>
                    <img class="img-fluid" src="~/Content/assets/img/mapcodepen526.jpg" alt="" />
                </div>
            </div>

            <!-- Portfolio Item React Form -->
            <div class="col-md-6 col-lg-4 mb-5 mb-md-0">
                <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal5">
                    <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                        <div class="portfolio-item-caption-content text-center text-white"><i class="fa fa-plus fa-3x" aria-hidden="true"></i></div>
                    </div>
                    <img class="img-fluid" src="~/Content/assets/img/business.jpg" alt="" />
                </div>
            </div>

            <!-- Portfolio Item FED -->
            <div class="col-md-6 col-lg-4 mb-5">
                <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal1">
                    <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                        <div class="portfolio-item-caption-content text-center text-white"><i class="fa fa-plus fa-3x" aria-hidden="true"></i></div>
                    </div>
                    <img class="img-fluid" src="~/Content/assets/img/genericPizzaLogoBlue.jpg" alt="" />
                </div>
            </div>

        </div>
